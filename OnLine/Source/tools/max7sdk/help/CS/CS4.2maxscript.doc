\par 
\par }{\i\f1\fs20\insrsid10165026 biped.bendFootprints <biped_ctrl> <float> --angle in degrees
\par biped.scaleFootprints <biped_ctrl> <float> --angle in degrees}{\f1\fs20\insrsid10165026 
